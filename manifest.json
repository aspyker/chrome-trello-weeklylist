{
  "manifest_version": 2,

  "name": "Trello Weekly List",
  "description": "This extension will show my weeks work in Trello.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Testing!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}